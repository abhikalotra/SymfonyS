function searchesLeft()
{

	
	$.ajax({
               	url : '{{ path("user_searchesLeft") }}',
                type:'POST',
               
                dataType:'html',   
                success:function(data)
		{

			var url = window.location.href;
		
			var base_url = window.location.origin;
			var curUrl =  document.URL;



			if( data == 'SUCCESS' && curUrl.indexOf('myPlan') < 0 && curUrl.indexOf('changePlan') < 0 && curUrl.indexOf('printChangePlan') < 0 && curUrl.indexOf('printRenewPlan') < 0 && curUrl.indexOf('lease/search') < 0 && curUrl.indexOf('drb/search') < 0 && curUrl.indexOf('user/userDRBDocuments') < 0 && curUrl.indexOf('user/viewDetailDRB') < 0 && curUrl.indexOf('user/userLeaseDocuments') < 0 && curUrl.indexOf('user/viewDetailLease') < 0)
			{
				$("#empty").css("display",'block');

			}
			else 
			{

				$("#empty").css("display",'none');
			}
		
		}
            });

}

function registrationType(id)
{

	
	$.ajax({
               	url : '{{ path("user_showRegistrationType") }}',
                type:'POST',
                data:'id='+id,
                dataType:'html',   
                success:function(data)
		{
			if( id == 'lease' )
                 		$('#leaseSubMenu').html(data);
			else if( id == 'drb' )
                 		$('#drbSubMenu').html(data);
		}
            });

}





