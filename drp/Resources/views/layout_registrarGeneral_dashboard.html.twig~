function registrationType(id)
{

	
	$.ajax({
               	url : '{{ path("registrarGeneral_showRegistrationType") }}',
                type:'POST',
                data:'id='+id,
                dataType:'html',   
                success:function(data)
		{
			if( id == 'lease' )
                 		$('#leaseSubMenu').html(data);
			else if( id == 'drb' )
                 		$('#drbSubMenu').html(data);
		}
            });

}



function showDeleteBox(url)
{
	$("#divDelete").show();
	$("#btnDel").attr('href',url);
}
function cancel()
{
	$("#divDelete").hide();
}

$(document).ready(function()
         		{
					 				
					 				var url = window.location.href;
					 				if( url.indexOf('/users') > -1 )
 									$("#listUser").addClass("start active open");
 								if( url.indexOf('/user/dashboard/statistics') > -1 )
 									$("#dashboard").addClass("start active open");
 								if( url.indexOf('/user/myPlan') > -1 )
 									$("#myplan").addClass("start active open");
 								if( url.indexOf('/showPlans') > -1 )
 									$("#plan").addClass("start active open");
 											if( url.indexOf('/managePayments') > -1 )
 									$("#payment").addClass("start active open");
 											if( url.indexOf('/manageLogs') > -1 )
 									$("#log").addClass("start active open");
 										if( url.indexOf('/searchQuota') > -1 )
 									$("#search").addClass("start active open");
 									if( url.indexOf('/instrumentType') > -1 )
 									$("#instrument").addClass("start active open");
 										if( url.indexOf('/businessType') > -1 )
 									$("#bType").addClass("start active open");
 									if( url.indexOf('/registrationType') > -1 )
 									$("#rType").addClass("start active open");
 								/*if( url.indexOf('dashboard') > -1 )
 									$("#Dashboard").addClass("active");
 								if( url.indexOf('dashboard') > -1 )
 									$("#Dashboard").addClass("active");
 								if( url.indexOf('dashboard') > -1 )
 									$("#Dashboard").addClass("active");
 								if( url.indexOf('dashboard') > -1 )
 									$("#Dashboard").addClass("active");*/
 						});

</script>


