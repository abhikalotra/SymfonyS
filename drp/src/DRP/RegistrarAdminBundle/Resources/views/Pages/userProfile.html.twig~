<script>
function back()
{
window.location.href = "{{ path('registrarAdmin_users') }}";

}
function changeStatus(id, currentStatus)
	{
		var arrId = id.split('-');
		id = arrId[1];
		$.ajax({
	   		 url: '{{ path("registrarAdmin_changeStatus") }}',
	   		 type: 'POST',
	   		 data: {id:id, currentStatus:currentStatus, objectType:'Payment'},
	   		 success:function(updatedStatus)
	   		 {
				$("#divStatus-"+id).html(updatedStatus);
				//location.reload();
	   		 }
	   	});
	}
</script>
	
