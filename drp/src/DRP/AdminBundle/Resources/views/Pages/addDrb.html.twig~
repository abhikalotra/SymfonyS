<script>
function showGrantor()
{
	
	$("#form2Grantor").show();
	$("#firstbuttonGrantor").hide();
	$("#secondbuttonGrantor").show();
	$("#firstRemoveGrantor").show();	
	
	//$(".drb").show();
}
function hideGrantor()
{
	$("#form2Grantor").hide();
	$("#firstbuttonGrantor").show();
	$("#secondbuttonGrantor").hide();
	$("#firstRemoveGrantor").hide();
}
function showGrantor2()
{
	$("#form3Grantor").show();
	$("#secondbuttonGrantor").hide();
	$("#firstRemoveGrantor").hide();	
	$("#secondRemoveGrantor").show();
}
function hideFormsGrantor()
{
	$("#form3Grantor").hide();
	$("#secondbuttonGrantor").show();
	$("#firstRemoveGrantor").show();
	$("#secondRemoveGrantor").hide();	
	
}
/*=========Grantee Starts here========*/
function showGrantee()
{
	
	$("#form2Grantee").show();
	$("#firstbuttonGrantee").hide();
	$("#secondbuttonGrantee").show();
	$("#firstRemoveGrantee").show();	
	
	//$(".drb").show();
}
function hideGrantee()
{
	$("#form2Grantee").hide();
	$("#firstbuttonGrantee").show();
	$("#secondbuttonGrantee").hide();
	$("#firstRemoveGrantee").hide();
}
function showGrantee2()
{
	$("#form3Grantee").show();
	$("#secondbuttonGrantee").hide();
	$("#firstRemoveGrantee").hide();	
	$("#secondRemoveGrantee").show();
}
function hideFormsGrantee()
{
	$("#form3Grantee").hide();
	$("#secondbuttonGrantee").show();
	$("#firstRemoveGrantee").show();
	
}

function showPR()
{
	
	$("#form2PR").show();
	$("#firstbuttonPR").hide();
	$("#secondbuttonPR").show();
	$("#firstRemovePR").show();	
	
	//$(".drb").show();
}
