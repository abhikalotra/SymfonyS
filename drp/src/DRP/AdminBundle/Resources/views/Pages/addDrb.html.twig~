<script src="https://maps.googleapis.com/maps/api/js?v=3.exp.&sensor=false&.js"></script>

<script>
function initialize() {
  var myLatlng = new google.maps.LatLng(13.422055,-15.3198853);
  var mapOptions = {
    zoom: 8,
    center: myLatlng
  }
  var map = new google.maps.Map(document.getElementById('map_canvas'), mapOptions);
var fx = google.maps.InfoWindow.prototype.setContent;


          //override the built-in setContent-method
          google.maps.InfoWindow.prototype.setContent = function (content) {
              //argument is a node
              if (content.querySelector) {

                  //search for the address
                  var addr = content.querySelector('.gm-basicinfo .gm-addr');

                  if (addr) {

                     
			$("#location").val(addr.textContent);

                  }
              }
              //run the original setContent-method
              fx.apply(this, arguments);
          };


  var marker = new google.maps.Marker({
      position: myLatlng,
      map: map,
      title: 'Gambia'
  });
}

google.maps.event.addDomListener(window, 'load', initialize);


</script>

 <script>
function checkSerialNumber(serial)
{

$.ajax({
               	url : '{{ path("drp_checkSerialNumber") }}',
                type:'POST',
                data:'serial='+serial,
                dataType:'html',   
                success:function(data){
			
                 	if(data == 'SUCCESS')
			{
				$("#exists").html('Serial Number already Exists')

			}
			else
			{
				$("#exists").html(' ')

			}		
                 
}
            });
}


function serialNumber(id)
{
	$("#ref").val(id);
$("#totalNumbers").hide();
}
function getReferenceNumber(value)
{

$("#totalNumbers").show();

$.ajax({
               	url : '{{ path("drp_getSerialNumber") }}',
                type:'POST',
                data:'value='+value,
                dataType:'html',   
                success:function(data){
			
                 	$('#totalNumbers').html(data);
			/*if(data!=value)
			{
				$("#sucess").html('Please Enter Correct Serial Number')
			}

                 	else
			{
				$("#sucess").hide();

			}*/
	}
		

            });
}






















